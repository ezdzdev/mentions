<% if !current_user.nil? %>
	<div class="form">
		<%= form_for :home, url: '/search' do |f| %>
		  	<%= f.label :'@'%>
		    <%= f.text_field :username, :placeholder=>"Search", :autocomplete=>"off"%>
		    <i class="fa fa-search" onclick="this.parentNode.submit();"></i>
		<% end %>
	</div>
<% end %>